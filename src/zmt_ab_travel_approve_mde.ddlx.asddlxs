    @UI.selectionField: [{ position: 50 }]
    @UI.lineItem: [{ position: 50 }]
    @UI.fieldGroup: [{ position: 20, qualifier: 'prices' }]
    @UI.dataPoint: {  qualifier: 'PriceData', title: 'Price Data' }
